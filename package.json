{
  "name": "@frsty/typesafe-query-keys",
  "version": "1.1.3",
  "description": "Type-safe query keys for TanStack Query",
  "type": "module",
  "author": "Sandesh Pandey",
  "main": ".src/runtime/index.ts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frstycodes/typesafe-query-keys/issues"
  },
  "homepage": "https://github.com/frstycodes/typesafe-query-keys#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/frstycodes/typesafe-query-keys.git"
  },
  "keywords": [
    "typescript",
    "tanstack-query",
    "query-keys",
    "type-safe",
    "vite-plugin",
    "webpack-plugin",
    "codegen"
  ],
  "bin": {
    "typesafe-query-keys": "./dist/cli/index.cjs"
  },
  "exports": {
    ".": {
      "import": "./dist/runtime/index.js",
      "require": "./dist/runtime/index.cjs"
    },
    "./plugin": {
      "import": "./dist/plugin/index.js",
      "require": "./dist/plugin/index.cjs"
    },
    "./plugin/vite": {
      "import": "./dist/plugin/vite/index.js",
      "require": "./dist/plugin/vite/index.cjs"
    },
    "./plugin/webpack": {
      "import": "./dist/plugin/webpack/index.js",
      "require": "./dist/plugin/webpack/index.cjs"
    },
    "./plugin/generic": {
      "import": "./dist/plugin/generic/index.js",
      "require": "./dist/plugin/generic/index.cjs"
    },
    "./plugin/generic/exec": {
      "import": "./dist/plugin/generic/exec.js",
      "require": "./dist/plugin/generic/exec.cjs"
    },
    "./config": {
      "import": "./dist/config/index.js",
      "require": "./dist/config/index.cjs"
    }
  },
  "scripts": {
    "build": "npm run clean && tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "ci:version": "npm run version",
    "ci:publish": "npm publish --no-git-checks",
    "prepublishOnly": "npm run build",
    "changeset": "cliff-jumper",
    "version": "cliff-jumper version",
    "release": "cliff-jumper release",
    "bump": "npm run changeset && npm run version && git push --follow-tags",
    "prepare": "husky",
    "commit": "cz"
  },
  "peerDependencies": {
    "typescript": ">=4",
    "vite": ">=2",
    "webpack": ">=3"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "cosmiconfig": "^9.0.0",
    "globby": "15.0.0",
    "globify-gitignore": "1.0.3",
    "neverthrow": "^8.2.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@favware/cliff-jumper": "6.0.0",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "husky": "9.1.7",
    "prettier": "3.6.2",
    "rimraf": "^6.0.1",
    "tsup": "8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "webpack": "5.102.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
